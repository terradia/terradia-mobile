{
  "expo": {
    "name": "terradia",
    "slug": "terradia",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "terradia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-terradia.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.terradia.terradiaApp"
    },
    "android": {
      "package": "com.terradia.terradiaApp",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB03Rz3UtCw0LzkDUYNZnmkQvn0ZwjX8PU"
        }
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "svg",
        "graphql"
      ]
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}