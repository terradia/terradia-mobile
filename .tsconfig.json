{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": [
      "dom",
      "dom-iterable",
      "esnext"
    ],
    "declaration": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "paths": {
    "@i18n/*": [
      "@i18n/*"
    ]
  }
}
